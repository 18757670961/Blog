<script>
    // only load tagcloud.js in tag.html
    if (jq("#tag_cloud").length !== 0) {
        async(
            "<%= config.url%><%= config.root%>js/jquery.tagcloud.js",
            function () {
                $.fn.tagcloud.defaults = {
                    //size: {start: 1, end: 1, unit: 'em'},
                    color: {
                        start: "#bbbbee",
                        end: "#0085a1",
                    },
                };
                jq("#tag_cloud a").tagcloud();
            }
        );
    }
</script>

<!-- Side Catalog -->
<% if (page.catalog) { %>
<script type="text/javascript">
    function generateCatalog(selector) {
        let P = jq("div.post-container"),
            a,
            n,
            t,
            l,
            i,
            c;
        a = P.find("h1,h2,h3,h4,h5,h6");
        a.each(function () {
            n = jq(this).prop("tagName").toLowerCase();
            i = "#" + jq(this).prop("id");
            t = jq(this).text();
            c = jq('<a href="' + i + '" rel="nofollow">' + t + "</a>");
            l = jq('<li class="' + n + '_nav"></li>').append(c);
            jq(selector).append(l);
        });
        return true;
    }

    generateCatalog(".catalog-body");

    // toggle side catalog
    jq(".catalog-toggle").click(function (e) {
        e.preventDefault();
        jq(".side-catalog").toggleClass("fold");
    });

    /*
     * Doc: https://github.com/davist11/jQuery-One-Page-Nav
     * Fork by Hux to support padding
     */
    async("<%= config.root%>js/jquery.nav.js", function () {
        jq(".catalog-body").onePageNav({
            currentClass: "active",
            changeHash: !1,
            easing: "swing",
            filter: "",
            scrollSpeed: 700,
            scrollOffset: 0,
            scrollThreshold: 0.2,
            begin: null,
            end: null,
            scrollChange: null,
            padding: 80,
        });
    });
</script>
<% } %>